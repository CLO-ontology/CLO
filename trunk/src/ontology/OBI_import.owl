        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></obo:IAO_0000119>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Establishment of HeLa immortal cell line</obo:IAO_0000112>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Helen Parkinson</rdfs:comment>
