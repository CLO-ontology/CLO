        <obo:IAO_0000111 xml:lang="en">organism</obo:IAO_0000111>
        <obo:IAO_0000112 xml:lang="en">plant</obo:IAO_0000112>
        <obo:IAO_0000112 xml:lang="en">virus</obo:IAO_0000112>
