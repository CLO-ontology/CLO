            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/OBI_0000299"/>
                <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/CLO_0000019"/>
